"Window": {
	backgroundColor: "#ededed",
	layout: "vertical"
},

"#nav": {
	width: "100%",
	height: 40,
	backgroundGradient: {
	    type: 'linear',
	    startPoint: { x: '0%', y: '0%' },
	    endPoint: { x: '0%', y: '100%' },
	    colors: [ { color: '#484848', offset: 0.0}, { color: '#373737', offset: 0.0 } ],
	},
	textAlign: "left"
},

".nav-camera": {
	color: "#fff",
	font: {
		fontSize: 14,
		fontWeight: 'normal',
		fontFamily: 'Helvetica Neue'
	}
}